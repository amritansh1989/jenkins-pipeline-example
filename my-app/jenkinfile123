pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'mkdir test'
                cd test
                sh 'git init'
                sh 'git pull https://github.com/amritansh1989/jenkins-pipeline-example.git'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing1..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
